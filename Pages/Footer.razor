<MudToolBar Dense="true" Class="--mud-palette-appbar-background mud-elevation-4">
    <MudSpacer/>
    <MudText Typo="Typo.caption" Align="Align.Center">
        Alexandre Alves Â© @yearInterval
    </MudText>
</MudToolBar>

@code {
    static ushort firstYear = 2023,
        currentYear = (ushort)DateTime.Now.Year;
    string yearInterval = currentYear == firstYear
        ? $"{firstYear}"
        : $"{firstYear}-{currentYear}";
}